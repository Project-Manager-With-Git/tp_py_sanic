{
    "language": "py",
    "env": "venv",
    "mini_language_version": "3.8",
    "description": "",
    "author": "mac",
    "template_type": "socket",
    "requires": [
        "sanic >= 22.9.0",
        "sanic-ext >= 22.9.0",
        "sanic-testing >= 22.9.0",
        "jsonschema >= 3.2.0",
        "pyloggerhelper >= 0.0.2",
        "schema-entry>=0.1.5"
    ],
    "test_requires": [
        "coverage >= 5.5",
        "mypy >= 0.800",
        "autopep8 >= 1.5.6",
        "pylint >= 2.8.0",
        "pydocstyle>=6.0.0",
        "requests >= 2.25.1",
        "aiounittest >= 1.4.0"
    ],
    "template_keys": {
        "app_name": {
            "description": "",
            "default": "{{ project_name }}"
        },
        "author": {
            "description": "作者",
            "default": "{{ author }}"
        },
        "author_email": {
            "description": "作者",
            "default": "{{ author_email }}"
        },
        "version": {
            "description": "项目版本",
            "default": "{{ version }}"
        },
        "description": {
            "description": "项目说明",
            "default": "{{ description }}"
        },
        "url":{
            "description": "项目地址",
            "default": "{{ url }}"
        },
        "source": {
            "description": "资源名",
            "default": "example"
        }
    },
    "components": {
        "decorator": {
            "source": "decorator",
            "description": "handler使用的装饰器组件",
            "default_path": "{{ project_name }}/decorator"
        },
        "decorator_source": {
            "source": "decorator/_ref_source_fer__decorator.py.jinja",
            "description": "handler使用的装饰器文件模版",
            "default_path": "{{ project_name }}/decorator/{{ source }}.py"
        },
        "download": {
            "source": "download",
            "description": "下载组件",
            "default_path": "{{ project_name }}/download"
        },
        "download_source": {
            "source": "download/_ref_source_fer__file.py.jinja",
            "description": "下载资源文件模版,多用于实时生成的文件",
            "default_path": "{{ project_name }}/download/{{ source }}_file.py"
        },
        "listener": {
            "source": "listener",
            "description": "sanic的listenner组件,用于挂载在sanic对象的声明周期中",
            "default_path": "{{ project_name }}/listener"
        },
        "listener_source": {
            "source": "listener/_ref_source_fer__listener.py.jinja",
            "description": "sanic的listenner文件模板",
            "default_path": "{{ project_name }}/listener/{{ source }}_listener.py"
        },
        "middleware": {
            "source": "middleware",
            "description": "sanic的自定义中间件组件,用于挂载在handler上",
            "default_path": "{{ project_name }}/middleware"
        },
        "middleware_reqsource": {
            "source": "middleware/_ref_source_fer__reqmw.py.jinja",
            "description": "sanic的自定义请求中间件文件模版",
            "default_path": "{{ project_name }}/middleware/{{ source }}_reqmw.py"
        },
        "middleware_ressource": {
            "source": "middleware/_ref_source_fer__resmw.py.jinja",
            "description": "sanic的自定义请求中间件文件模版",
            "default_path": "{{ project_name }}/middleware/{{ source }}_resmw.py"
        },
        "rest": {
            "source": "rest",
            "description": "sanic的RESTful接口组件",
            "default_path": "{{ project_name }}/rest"
        },
        "rest_source": {
            "source": "rest/_ref_source_fer__source",
            "description": "sanic的RESTful接口中资源接口模版",
            "default_path": "{{ project_name }}/rest/{{ source }}_source"
        },
        "sse": {
            "source": "sse",
            "description": "sanic的sse接口组件",
            "default_path": "{{ project_name }}/sse"
        },
        "sse_source": {
            "source": "sse/_ref_source_fer__source.py.jinja",
            "description": "sanic的sse接口中资源接口模版",
            "default_path": "{{ project_name }}/sse/{{ source }}_source.py"
        },
        "view": {
            "source": "view",
            "description": "sanic的动态页面组件",
            "default_path": "{{ project_name }}/view"
        },
        "view_source": {
            "source": "view/_ref_source_fer__view.py.jinja",
            "description": "sanic的动态页面文件模版",
            "default_path": "{{ project_name }}/view/{{ source }}_view.py"
        },
        "ws": {
            "source": "ws",
            "description": "sanic的websocket接口组件",
            "default_path": "{{ project_name }}/ws"
        },
        "ws_source": {
            "source": "ws/_ref_source_fer__source.py.jinja",
            "description": "sanic的websocket接口中资源接口模版",
            "default_path": "{{ project_name }}/ws/{{ source }}_source.py"
        },
        "main": {
            "source": "__main__.py.jinja",
            "description": "sanic项目的入口模块组件",
            "default_path": "{{ project_name }}/__main__.py"
        },
        "dockerfile": {
            "source": "dockerfile.jinja",
            "description": "",
            "default_path": "dockerfile"
        },
        "servapi": {
            "source": "servs/servapi.py.jinja",
            "description": "包含RESTful接口,sse接口,和websocket接口的服务模版",
            "default_path": "{{ project_name }}/serv.py"
        },
        "servmvc": {
            "source": "servs/servmvc.py.jinja",
            "description": "由jinja2模版动态渲染的应用模版",
            "default_path":"{{ project_name }}/serv.py"
        },
        "servmvpapi": {
            "source": "servs/servmvpapi.py.jinja",
            "description": "由jinja2模版动态渲染的应用模版同时包含RESTful接口,sse接口,和websocket接口的服务模版",
            "default_path": "{{ project_name }}/serv.py"
        },
        "servrest": {
            "source": "servs/servrest.py.jinja",
            "description": "RESTfulapi服务模版",
            "default_path": "{{ project_name }}/serv.py"
        },
        "servspa": {
            "source": "servs/servspa.py.jinja",
            "description": "混合RESTful,ws,sse和download的单页应用模版",
            "default_path": "{{ project_name }}/serv.py"
        },
        "servstatic": {
            "source": "servs/servstatic.py.jinja",
            "description": "静态资源服务模板",
            "default_path": "{{ project_name }}/serv.py"
        },
        "servws": {
            "source": "servs/servws.py.jinja",
            "description": "websocket服务模版",
            "default_path": "{{ project_name }}/serv.py"
        }
    }
}